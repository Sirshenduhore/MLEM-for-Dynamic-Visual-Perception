def calculateAvgEmotion(emotions):
	total_frames=sum(emotions.values())
	for key,value in emotions.items():
		percent=value*100.0/total_frames
		ans="\n{}:\tPercentage:{}%\n"
		print(ans.format(key,round(percent,2)))
	print(total_frames)


